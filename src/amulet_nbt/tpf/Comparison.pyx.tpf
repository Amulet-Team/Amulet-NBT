{{
"\n\n".join(
    f"""    def {meth}({py_cls} self, other):
        if not isinstance(other, {py_cls}):
            return NotImplemented
        cdef {py_cls} other_ = other
        return self.cpp {op} other_.cpp"""
    for meth, op, py_cls in (
        ("__ge__", ">=", py_cls),
        ("__gt__", ">", py_cls),
        ("__le__", "<=", py_cls),
        ("__lt__", "<", py_cls),
    )
)
